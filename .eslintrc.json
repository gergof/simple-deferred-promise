{
	"parser": "@typescript-eslint/parser",
	"env": {
		"es2021": true,
		"jest": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/recommended",
		"prettier"
	],
	"plugins": ["@typescript-eslint", "prettier"],
	"parserOptions": {
		"ecmaVersion": 12,
		"project": "./tsconfig.json",
		"tsconfigRootDir": "."
	},
	"rules": {
		"no-console": 2,
		"prettier/prettier": 2,
		"@typescript-eslint/no-explicit-any": 0
	}
}
